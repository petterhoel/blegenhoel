  "devDependencies": {
    "npm-run-all": "4.1.5",
    "prettier": "2.8.3",
    "prettier-plugin-astro": "0.8.0",
    "rimraf": "4.1.2",
    "sass": "1.58.0",
    "typescript": "^4.0.0",
    "vitest": "0.28.3"
  }
